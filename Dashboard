        self.label.setFont(font)
        self.label.setAlignment(Qt.AlignCenter)
        self.label.setText(QCoreApplication.translate("MainWindow", u"Cyber Security", None))
        self.label.setStyleSheet(u"border: none;")


        self.label = QLabel(self.frame)  # Add a QLabel widget to hold the image
        self.label.setObjectName(u"label")
        self.label.setGeometry(QRect(20, 20, 261, 160))  # Adjusted label geometry
        pixmap = QPixmap("one.jpg")  # Set the image using QPixmap
        scaled_pixmap = pixmap.scaled(261, 261, Qt.AspectRatioMode.KeepAspectRatio)  # Resize the image
        self.label.setPixmap(scaled_pixmap)  # Set the scaled image
        self.pushButton = QPushButton(self.frame)
        self.pushButton.setObjectName(u"pushButton")
        self.pushButton.setGeometry(QRect(30, 240, 111, 41))
        font3 = QFont()
        font3.setFamilies([u"Times New Roman"])
        font3.setPointSize(16)
        self.pushButton.setFont(font3)
        self.pushButton.setStyleSheet(u"\n"
                                        "QPushButton {\n"
                                        "	background-color: rgb(12, 12, 12);\n"
